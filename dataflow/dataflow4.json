			"scriptLines": [
				"source(output(",
				"          BroadcastYears as (Season as (NielsenEndDate as date, NielsenStartDate as date, STBSEndDate as date, STBSStartDate as date, Years as string)[])",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
